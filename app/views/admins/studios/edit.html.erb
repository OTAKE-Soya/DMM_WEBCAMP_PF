<%= form_with model: @studio, url: admins_studio_path, local: true do |f| %>
  <%= f.label "スタジオ名" %>
  <%= f.text_field :name %>
  
  <%= f.label "平日1時間当たり料金"%>
  <%= f.text_field :weekday_fee %>
  
  <%= f.label "土日祝日1時間当たり料金" %>
  <%= f.text_field :weekend_fee %>
  
  <%= f.label "広さ" %>
  <%= f.text_field :area %>帖
  
  <%= f.submit %>
<% end %>