<div class="container">
  <div class="row">
    <h2>会員一覧</h2>
  </div>
  <div class="row">
    <table class="table">
      <tr>
        <th>会員番号</th>
        <th>名前</th>
        <th>退会状況</th>
      </tr>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <td><%= link_to user.name, admins_user_path(user) %></td>
          <th><%= user.status %></th>
        </tr>
      <% end %>
    </table>
  </div>
</div>



